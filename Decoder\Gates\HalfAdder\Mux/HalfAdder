S_0x55ecd6202d20 .scope module, "HalfAddTest_v" "HalfAddTest_v" 2 26;
 .timescale -9 -12;
v0x55ecd6259fd0_0 .var "A", 0 0;
v0x55ecd625a0c0_0 .var "B", 0 0;
v0x55ecd625a190_0 .net "Cout", 0 0, L_0x55ecd625a820;  1 drivers
v0x55ecd625a290_0 .net "Sum", 0 0, L_0x55ecd625a690;  1 drivers
v0x55ecd625a360_0 .var "passed", 7 0;
S_0x55ecd6202eb0 .scope task, "allPassed" "allPassed" 2 42, 2 42 0, S_0x55ecd6202d20;
 .timescale -9 -12;
v0x55ecd6203090_0 .var "numTests", 7 0;
v0x55ecd6259160_0 .var "passed", 7 0;
TD_HalfAddTest_v.allPassed ;
    %load/vec4 v0x55ecd6259160_0;
    %load/vec4 v0x55ecd6203090_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 46 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 47 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0x55ecd6259240 .scope module, "dut" "HalfAdd" 2 60, 3 1 0, S_0x55ecd6202d20;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "Cout";
    .port_info 1 /OUTPUT 1 "Sum";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x55ecd625a450 .functor NAND 1, v0x55ecd6259fd0_0, v0x55ecd625a0c0_0, C4<1>, C4<1>;
L_0x55ecd625a560 .functor NAND 1, L_0x55ecd625a450, v0x55ecd6259fd0_0, C4<1>, C4<1>;
L_0x55ecd625a620 .functor NAND 1, L_0x55ecd625a450, v0x55ecd625a0c0_0, C4<1>, C4<1>;
L_0x55ecd625a690 .functor NAND 1, L_0x55ecd625a560, L_0x55ecd625a620, C4<1>, C4<1>;
L_0x55ecd625a820 .functor NAND 1, L_0x55ecd625a450, L_0x55ecd625a450, C4<1>, C4<1>;
v0x55ecd6259440_0 .net "A", 0 0, v0x55ecd6259fd0_0;  1 drivers
v0x55ecd6259500_0 .net "B", 0 0, v0x55ecd625a0c0_0;  1 drivers
v0x55ecd62595c0_0 .net "Cout", 0 0, L_0x55ecd625a820;  alias, 1 drivers
v0x55ecd6259690_0 .net "Sum", 0 0, L_0x55ecd625a690;  alias, 1 drivers
v0x55ecd6259750_0 .net "xor1", 0 0, L_0x55ecd625a450;  1 drivers
v0x55ecd6259860_0 .net "xor2", 0 0, L_0x55ecd625a560;  1 drivers
v0x55ecd6259920_0 .net "xor3", 0 0, L_0x55ecd625a620;  1 drivers
S_0x55ecd6259a60 .scope task, "passTest" "passTest" 2 33, 2 33 0, S_0x55ecd6202d20;
 .timescale -9 -12;
v0x55ecd6259c40_0 .var "actualOut", 1 0;
v0x55ecd6259d20_0 .var "expectedOut", 1 0;
v0x55ecd6259e00_0 .var "passed", 7 0;
v0x55ecd6259ef0_0 .var "testType", 120 0;
TD_HalfAddTest_v.passTest ;
    %load/vec4 v0x55ecd6259c40_0;
    %load/vec4 v0x55ecd6259d20_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 38 "$display", "%s passed", v0x55ecd6259ef0_0 {0 0 0};
    %load/vec4 v0x55ecd6259e00_0;
    %addi 1, 0, 8;
    %store/vec4 v0x55ecd6259e00_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 39 "$display", "%s failed: %d should be %d", v0x55ecd6259ef0_0, v0x55ecd6259c40_0, v0x55ecd6259d20_0 {0 0 0};
T_1.3 ;
    %end;
    .scope S_0x55ecd6202d20;
T_2 ;
    %vpi_call 2 29 "$dumpfile", "HalfAdder.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55ecd6202d20 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55ecd6202d20;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecd6259fd0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecd625a0c0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55ecd625a360_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecd6259fd0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecd625a0c0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x55ecd625a190_0;
    %load/vec4 v0x55ecd625a290_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x55ecd6259c40_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x55ecd6259d20_0, 0, 2;
    %pushi/vec4 3156784, 0, 121;
    %store/vec4 v0x55ecd6259ef0_0, 0, 121;
    %load/vec4 v0x55ecd625a360_0;
    %store/vec4 v0x55ecd6259e00_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x55ecd6259a60;
    %join;
    %load/vec4 v0x55ecd6259e00_0;
    %store/vec4 v0x55ecd625a360_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 1;
